{
  "manifest_version": 3,
  "name": "oneTwoOne",
  "short_name": "oneTwoOne",
  "description": "Defines and manages who should be able to sign into a device",
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "minimum_chrome_version": "66",
  "permissions": [
    "identity",
    "identity.email",
    "enterprise.deviceAttributes",
    "activeTab",
    "webRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}